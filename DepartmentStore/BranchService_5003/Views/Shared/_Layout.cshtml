<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"]</title>
    <link rel="stylesheet" href="~/branch/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/branch/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/branch/BranchService_5003.styles.css" asp-append-version="true" />
</head>
<body>
    <div class="container">
        <main role="main" class="pb-3 d-flex align-items-center justify-content-center">
            @RenderBody()
        </main>
    </div>
    <script src="~/branch/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/branch/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/branch/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
